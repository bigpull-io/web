{
  "name": "my-qwik-basic-starter",
  "description": "Demo App with Routing built-in (recommended)",
  "engines": {
    "node": "18.x"
  },
  "type": "module",
  "private": true,
  "scripts": {
    "prepare": "panda codegen",
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/vercel-edge/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "vercel deploy",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "----- TOOLS": "-----",
    "tt": "ts-node --esm src/tools/mplus-s4-bis.ts",
    "tools": "npm-run-all --parallel tool:*",
    "tool:item-craft-levels": "ts-node --esm src/tools/itemCraftLevels.ts",
    "tool:item-qualities": "ts-node --esm src/tools/itemQualities.ts",
    "tool:item-upgrade-trakcs": "ts-node --esm src/tools/itemUpgradeTracks.ts",
    "tool:mplus-s4-loot": "ts-node --esm src/tools/mplus-s4-loot.ts"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.17",
    "@builder.io/qwik-city": "^1.2.17",
    "@pandacss/dev": "^0.18.2",
    "@sentry/browser": "^7.80.0",
    "@trivago/prettier-plugin-sort-imports": "^4.3.0",
    "@types/eslint": "8.44.7",
    "@types/node": "^20.9.0",
    "@typescript-eslint/eslint-plugin": "6.10.0",
    "@typescript-eslint/parser": "6.10.0",
    "@vercel/analytics": "^1.1.1",
    "eslint": "8.53.0",
    "eslint-plugin-qwik": "^1.2.17",
    "fast-xml-parser": "^4.3.2",
    "npm-run-all": "^4.1.5",
    "p-queue": "^7.4.1",
    "prettier": "3.1.0",
    "typescript": "5.2.2",
    "undici": "5.27.2",
    "vercel": "^32.5.3",
    "vite": "4.5.0",
    "vite-tsconfig-paths": "4.2.1"
  }
}
