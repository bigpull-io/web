{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "jsxSingleQuote": false,
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",

  "importOrder": ["<THIRD_PARTY_MODULES>", "^~", "^../", "^./"],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
